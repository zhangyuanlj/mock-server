{
    "restartable": "rs",
    "ignore": [
        ".git",
        ".svn",
        "node_modules/**/node_modules",
        "src"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "watch": [
        "mock"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json"
}